{
  "private": true,
  "license": "BSD-3-Clause",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "fix": "npm-run-all --continue-on-error fix:*",
    "fix:eslint": "eslint --fix \"**/*\"",
    "fix:markdownlint": "markdownlint --fix \"**/*\"",
    "fix:prettier": "prettier --write \"**/*\"",
    "fix:yarn-deduplicate": "yarn install && yarn-deduplicate --strategy=fewer && yarn install",
    "lint": "npm-run-all --continue-on-error lint:*",
    "lint:eslint": "eslint \"**/*\" --report-unused-disable-directives",
    "lint:markdownlint": "markdownlint \"**/*\"",
    "lint:prettier": "prettier --check \"**/*\"",
    "lint:tsc": "tsc --project .",
    "lint:yarn-deduplicate": "yarn-deduplicate --fail --list --strategy=fewer",
    "start": "next start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "run-if-changed",
      "post-checkout": "run-if-changed",
      "post-merge": "run-if-changed",
      "post-rewrite": "run-if-changed"
    }
  },
  "lint-staged": {
    "**/*": [
      "eslint --fix",
      "markdownlint --fix",
      "prettier --write"
    ]
  },
  "resolutions": {
    "node-fetch": ">=2.6.1"
  },
  "dependencies": {
    "@visx/axis": "^1.0.0",
    "@visx/clip-path": "^1.0.0",
    "@visx/grid": "^1.0.0",
    "@visx/scale": "^1.0.0",
    "accept-language-parser": "^1.5.0",
    "expr-eval": "^2.0.2",
    "i18next": "^19.7.0",
    "i18next-icu": "^1.4.2",
    "immer": "^7.0.8",
    "lru-cache": "^6.0.0",
    "next": "^9.5.4",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.7.2",
    "react-is": "^16.13.1",
    "react-use": "^15.3.3",
    "styled-components": "^5.1.1",
    "styled-normalize": "^8.0.7",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@hkdobrev/run-if-changed": "^0.3.1",
    "@kachkaev/eslint-config-react": "^0.1.1",
    "@kachkaev/markdownlint-config": "^0.1.0",
    "@kachkaev/prettier-config": "^1.0.0",
    "@next/bundle-analyzer": "^9.5.2",
    "@types/accept-language-parser": "^1.5.1",
    "@types/lru-cache": "^5.1.0",
    "@types/node": "^14.6.2",
    "@types/styled-components": "^5.1.2",
    "@types/uuid": "^8.3.0",
    "eslint": "^7.7.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.13",
    "markdownlint-cli": "^0.23.2",
    "next-compose-plugins": "^2.2.0",
    "prettier": "^2.1.1",
    "typescript": "^3.9.7",
    "yarn-deduplicate": "^3.0.0"
  },
  "run-if-changed": {
    "yarn.lock": "yarn install --prefer-offline --pure-lockfile"
  }
}
