{
  "private": true,
  "license": "BSD-3-Clause",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "fix": "npm-run-all --continue-on-error fix:*",
    "fix:eslint": "eslint --fix \"**/*\"",
    "fix:markdownlint": "markdownlint --fix \"**/*\"",
    "fix:prettier": "prettier --write \"**/*\"",
    "fix:yarn-deduplicate": "yarn install && yarn-deduplicate --strategy=fewer && yarn install",
    "lint": "npm-run-all --continue-on-error lint:*",
    "lint:eslint": "eslint \"**/*\" --report-unused-disable-directives",
    "lint:markdownlint": "markdownlint \"**/*\"",
    "lint:prettier": "prettier --check \"**/*\"",
    "lint:tsc": "tsc --project .",
    "lint:yarn-deduplicate": "yarn-deduplicate --fail --list --strategy=fewer",
    "start": "next start",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*": [
      "eslint --fix",
      "markdownlint --fix",
      "prettier --write"
    ]
  },
  "resolutions": {
    "node-fetch": ">=2.6.1"
  },
  "dependencies": {
    "@visx/axis": "^1.17.1",
    "@visx/clip-path": "^1.7.0",
    "@visx/grid": "^1.17.1",
    "@visx/scale": "^1.14.0",
    "accept-language-parser": "^1.5.0",
    "expr-eval": "^2.0.2",
    "i18next": "^20.4.0",
    "i18next-icu": "^2.0.3",
    "immer": "^9.0.5",
    "intl-messageformat": "^9.9.0",
    "lru-cache": "^6.0.0",
    "next": "^11.1.1",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.11.4",
    "react-is": "^17.0.2",
    "react-use": "^17.2.4",
    "styled-components": "^5.3.0",
    "styled-normalize": "^8.0.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@hkdobrev/run-if-changed": "^0.3.1",
    "@kachkaev/eslint-config-react": "^0.2.3",
    "@kachkaev/markdownlint-config": "^0.2.0",
    "@kachkaev/prettier-config": "^1.2.0",
    "@next/bundle-analyzer": "^11.1.0",
    "@types/accept-language-parser": "^1.5.2",
    "@types/lru-cache": "^5.1.1",
    "@types/node": "^16.6.1",
    "@types/styled-components": "^5.1.12",
    "@types/uuid": "^8.3.1",
    "eslint": "^7.32.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.2",
    "markdownlint-cli": "^0.28.1",
    "next-compose-plugins": "^2.2.1",
    "prettier": "~2.2.1",
    "typescript": "^4.3.5",
    "yarn-deduplicate": "^3.1.0"
  },
  "run-if-changed": {
    "yarn.lock": "yarn install --prefer-offline --pure-lockfile"
  }
}
